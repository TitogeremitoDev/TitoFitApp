      "EXPO_PUBLIC_GOOGLE_ANDROID_CLIENT_ID_PROD": "915310517535-gkntma85f58tulhaedc2gsalslg0hp7a.apps.googleusercontent.com",
      "EXPO_PUBLIC_APP_ENV": "dev"
    },
    "updates": {
