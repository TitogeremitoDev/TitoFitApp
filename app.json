{
  "expo": {
    "name": "TotalGains",
    "slug": "TitoFitApp",
    "version": "0.9.0",
    "runtimeVersion": "0.9.0",
    "sdkVersion": "53.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/mi-logo-icono.png",
    "scheme": "com.german92.titofitapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/wait.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.german92.titofitapp",
      "usesAppleSignIn": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/mi-logo-icono-foreground.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.german92.titofitapp",
      "softwareKeyboardLayoutMode": "resize",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "com.german92.titofitapp",
              "host": "oauthredirect"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "baseUrl": "/app"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/wait.png",
          "resizeMode": "cover",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-notifications",
      "expo-secure-store",
      "expo-apple-authentication",
      [
        "expo-build-properties",
        {
          "android": {
            "newArchEnabled": false
          }
        }
      ],
      [
        "expo-updates",
        {
          "runtimeVersion": "0.9.0"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "baseUrl": "/app"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3233332a-d64c-4f96-9775-d8043263302c"
      },
      "EXPO_PUBLIC_API_URL": "https://consistent-donna-titogeremito-29c943bc.koyeb.app",
      "EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID": "915310517535-u0n1rrrvjf9bnkjc0b0a8kj7qogoe18i.apps.googleusercontent.com",
